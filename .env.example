GCP_SERVICE_ACCOUNT_KEY="ewo...<base64 key>...=="
LLM_CONTEXT_SIZE=200000
LLM_WORD_LIMIT=150000
LLM_TEMPERATURE=0.9
LLM_MAX_TOKENS=8000
